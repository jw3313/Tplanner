<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mem">
	
	<!-- 원글 저장 -->
	<insert id="addMem" parameterType="com.tplanner.vo.MemVO">
		INSERT INTO member_t(id, pw, phone, email, status)
		VALUES(#{id}, #{pw}, #{phone}, #{email}, 0)
	</insert>
	
</mapper>